{
  "name": "ifonnyapi",
  "version": "1.0.0",
  "description": "iFonny.fr API",
  "main": "app.js",
  "scripts": {
    "start": "node app.js"
  },
  "apps": [{
    "name": "ifonny-api",
    "script": "./app.js",
    "restart_delay": 1000,
    "watch": true,
    "ignore_watch": ["node_modules", "*.log", ".git"],
    "post_update": ["npm install"],
    "log_date_format": "YYYY-MM-DD HH:mm Z",
    "env": {
      "NODE_ENV": "dev"
    },
    "env_production": {
      "NODE_ENV": "prod"
    }
  }],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iFonny/iFonnyAPI.git"
  },
  "keywords": [
    "Api"
  ],
  "author": "iFonny",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/iFonny/iFonnyAPI/issues"
  },
  "homepage": "https://github.com/iFonny/iFonnyAPI#readme",
  "dependencies": {
    "request": "^2.81.0",
    "restify": "^5.0.1"
  }
}